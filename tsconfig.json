{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": ".vercel/output",
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": [
    "api/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "plugin"
  ]
}