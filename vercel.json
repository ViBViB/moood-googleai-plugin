{
  "version": 2,
  "builds": [
    {
      "src": "api/login.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/callback.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/login",
      "dest": "/api/login.ts"
    },
    {
      "src": "/api/callback",
      "dest": "/api/callback.ts"
    },
    {
      "src": "/auth-success.html",
      "dest": "/public/auth-success.html"
    }
  ]
}